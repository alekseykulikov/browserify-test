    "subarg": "^1.0.0",
    "testem": "^0.9.9",
    "watchify": "^3.6.0"
  },
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.2",
    "babel-eslint": "^4.1.4",
    "babel-preset-es2015": "^6.1.2",
    "babelify": "^7.2.0",
    "browserify-handlebars": "^1.0.0",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "mocha": "^2.3.3"
