{
  "name": "browserify-test",
  "version": "2.2.0",
  "description": "Test front-end libraries using browserify and mocha with ease",
  "license": "MIT",
  "repository": "alekseykulikov/browserify-test",
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "files": [
    "lib",
    "public"
  ],
  "keywords": [
    "browserify",
    "watchify",
    "testem",
    "mocha",
    "test",
    "front-end",
    "development"
  ],
  "scripts": {
    "test": "npm run prepublish && eslint src/ test/ && mocha",
    "prepublish": "babel src --out-dir lib --presets es2015 && chmod 755 lib/cli.js"
  },
  "dependencies": {
    "browserify": "^13.1.0",
    "commander": "^2.9.0",
    "concat-stream": "^1.5.1",
    "errorify": "^0.3.1",
    "glob": "^7.0.5",
    "lodash.omit": "^4.4.1",
    "subarg": "^1.0.0",
    "testem": "^1.10.3",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babelify": "^7.3.0",
    "browserify-handlebars": "^1.0.0",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.5",
    "handlebars": "^4.0.5",
    "mocha": "^3.0.2"
  }
}
