{
  "name": "browserify-test",
  "version": "2.0.0",
  "description": "Test front-end libraries using browserify and mocha with ease",
  "license": "MIT",
  "repository": "alekseykulikov/browserify-test",
  "files": [
    "src"
  ],
  "main": "./src/index.js",
  "bin": "./src/cli.js",
  "keywords": [
    "browserify",
    "watchify",
    "testem",
    "mocha",
    "test",
    "front-end",
    "development"
  ],
  "scripts": {
    "test": "npm run prepublish && jshint lib/ test/ && jscs lib/ test/ && mocha",
