{
  "name": "browserify-test",
  "description": "An easy way to test front-end libraries using browserify and mocha",
  "repository": "git://github.com/alekseykulikov/browserify-test.git",
  "version": "1.3.0",
  "license": "MIT",
  "files": [
    "src"
  ],
  "main": "./src/index.js",
  "bin": "./src/cli.js",
  "scripts": {
    "test": "npm run prepublish && jshint lib/ test/ && jscs lib/ test/ && mocha",
    "prepublish": "babel lib -d src && chmod 755 src/cli.js"
  },
  "dependencies": {
    "browserify": "^10.2.4",
    "commander": "^2.8.1",
    "glob": "^5.0.13",
    "testem": "^0.9.0-1",
    "watchify": "^3.2.3"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "chai": "^3.0.0",
    "mocha": "^2.2.5"
  },
    "babel": "^5.8.23",
    "jscs": "^2.1.1",
    "jshint": "^2.8.0",
  }
}
