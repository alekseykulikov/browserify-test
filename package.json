{
  "name": "browserify-test",
  "version": "3.1.0",
  "description": "Test front-end libraries using browserify and mocha with ease",
  "license": "MIT",
  "repository": "alekseykulikov/browserify-test",
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "lib",
    "public"
  ],
  "keywords": [
    "browserify",
    "watchify",
    "testem",
    "mocha",
    "test",
    "front-end",
    "development"
  ],
  "scripts": {
    "test": "npm run prepublish && eslint src/ test/ && mocha test/index.js test/cli.js",
    "prepublish": "babel src --out-dir lib --presets es2015"
  },
  "dependencies": {
    "browserify": "^14.1.0",
    "commander": "^2.9.0",
    "concat-stream": "^1.6.0",
    "errorify": "^0.3.1",
    "glob": "^7.1.1",
    "lodash.omit": "^4.5.0",
    "subarg": "^1.0.0",
    "testem": "^1.15.0",
    "watchify": "^3.9.0",
    "phantomjs-prebuilt": "^2.1.14"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babelify": "^7.3.0",
    "browserify-handlebars": "^1.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "handlebars": "^4.0.6",
    "mocha": "^3.2.0"
  }
}
