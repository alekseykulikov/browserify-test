    "commander": "^2.9.0",
    "concat-stream": "^1.5.1",
    "debug": "^2.2.0",
    "errorify": "^0.3.1",
    "glob": "^5.0.15",
    "testem": "^0.9.9",
    "watchify": "^3.6.0"
  },
  "devDependencies": {
    "babel": "^6.0.15",
    "babelify": "^7.2.0",
    "chai": "^3.4.1",
    "jscs": "^2.1.1",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3"
  }
}
